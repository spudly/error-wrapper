{
  "name": "error-wrapper",
  "version": "2.0.1",
  "description": "A base class for errors that wrap other errors.",
  "main": "umd.js",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.0.0",
    "eslint": "^3.0.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "nyc": "^6.6.1",
    "semistandard": "^8.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "test": "nyc --require babel-register mocha -u exports",
    "build": "browserify -t [ babelify --presets [ es2015 ] ] src/ErrorWrapper.js --standalone ErrorWrapper > umd.js",
    "lint": "jshint src && semistandard",
    "prepublish": "npm run build"
  },
  "jshintConfig": {
    "esnext": true
  },
  "author": "Stephen Sorensen <shuoink@gmail.com> (http://www.stephenjohnsorensen.com/)",
  "license": "MIT",
  "dependencies": {
    "error-subclass": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spudly/error-wrapper.git"
  },
  "files": [
    "umd.js"
  ]
}
